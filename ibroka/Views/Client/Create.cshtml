                                                                    </div>

                                                                </div>
                                                                <div class="col-md-4">

                                                                    <div class="form-group">
                                                                        <label>Email</label>
                                                                        <input asp-for="Corporate.Email" class="form-control" />
                                                                    </div>
                                                                </div>
                                                            </div>


                                                            <div class="row">
                                                                <div class="col-md-4">
                                                                    <div class="form-group">
                                                                        <label>Phone No</label>
                                                                        <input asp-for="Corporate.PhoneNo" class="form-control" />
